    <% if policy(@kitty).destroy? %>
      <%= link_to 'Destroy', @kitty, method: :delete, :class=>"teach-btn btn button-landing sticky-bottom", data: { confirm: 'Are you sure you want to delete this lovely kitty ???' } %>
    <% end %>
